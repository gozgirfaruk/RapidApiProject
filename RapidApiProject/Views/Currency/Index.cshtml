@model List<ExchangeViewModel.Exchange_Rates>
@{
    Layout = null;
}
<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
<div class="container">

<table class="table table-bordered">
    <tr>
        <th>Döviz</th>
        <th>Kur</th>
    </tr>

    @foreach(var item in Model)
    {
        <tr>
            <td>@item.currency</td>
            <td>@item.exchange_rate_buy</td>
        </tr>
    }
</table> 
</div>